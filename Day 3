#include <iostream>
using namespace std;

int main (){
  // print the square 
    int num;
    int count = 1;
    cout <<"Enter the number: ";
    cin >> num;

    for(int i = 1; i <= num; i++){
        cout << i*i;
        count += 1;
    }

    int package;
    cin >> package;

    if(package > 10)
    {
        cout <<"Accepted";
    }
    else 
    {
        cout <<"Rejected";
    }

  // using if else and more
    int marks;
    cout <<"Enters Your Marks: ";
    cin >> marks;

    if(marks > 33)
    {
    cout <<"Pass";
    }
    else{
        cout <<"Fail";
    }

    int a,b;
    cout <<"Enter first Number: ";
    cin >> a;
    cout <<"Enter your second number: ";
    cin >> b;

    if(a > b){
        cout <<"Yes ";
    }
    else {
        cout << "No"; 
    }

    int number;
    cout <<"Enter the number: ";
    cin >> number;

    if(number%2 == 0){
        cout << "Even";
    }

    else{
        cout << "Odd";
    }

    if(number > 18){
        cout <<"Adult";
    }
    else {
        cout <<"Teenager";
    }
  // take input print the output 
  int a,b;
    cout << "Enter your 1st number: ";
    cin >> a;
    cout << "Enter your 2nd number: ";
    cin >> b;
    cout << "Sum of two Number is: "<< a+b << endl;


// Type casting program 
   int a = 10;
    char c = 'A';
    a = c;
    cout << a;
    //this all process is called type casting

}
